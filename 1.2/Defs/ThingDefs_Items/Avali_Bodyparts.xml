  <ThingDef ParentName="LeatherBase">
    <defName>Avali_Leather</defName>
    <label>Avali Plumage</label>
    <description>Soft Plumage from an Avali, surprisingly warm.</description>
    <graphicData>
      <color>(128, 128, 128)</color>
    </graphicData>
    <statBases>
      <MarketValue>10</MarketValue>
      <StuffPower_Armor_Sharp>0.35</StuffPower_Armor_Sharp>
      <StuffPower_Armor_Blunt>0.35</StuffPower_Armor_Blunt>
      <StuffPower_Insulation_Cold>35</StuffPower_Insulation_Cold>
      <StuffPower_Insulation_Heat>15</StuffPower_Insulation_Heat>
    </statBases>
    <stuffProps>
      <color>(128, 128, 128)</color>
      <commonality>0.001</commonality>
      <statFactors>
        <MaxHitPoints>1.0</MaxHitPoints>
      </statFactors>
    </stuffProps>
  </ThingDef>
